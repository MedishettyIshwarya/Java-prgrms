//write a program to find the sum of digits of a number
public class sumofdigits{
    public static int sum(int[]a){
        int sum=0;
        for(int i=0;i<a.length;i++){
            sum+=a[i];   
        }
        return sum;
    }
public static void main(String[]args){
    int a[]={1,2,3,4};
    System.out.println(sum(a));
}
        

        

    }
